  dialogue:
    sentences:
    - speakerName: Onlinda
      sentenceText: "Hey you \u201Csleuths\u201D. If you\u2019re still looking for
        info about the Krampus, might I suggest heading over to Karl or Redmond\u2019s
        place."
      talkerImage: {fileID: 21300000, guid: 03e5d331ed724f64982737d88f640932, type: 3}
      introduceSpeaker: 1
      isMC: 0
      removeSpeaker: 
    - speakerName: Felix
      sentenceText: "Who\u2019re they?"
      talkerImage: {fileID: 21300000, guid: c8b92b8c5468c3c41b25cf7656decb72, type: 3}
      introduceSpeaker: 1
      isMC: 1
      removeSpeaker: 
    - speakerName: Onlinda
      sentenceText: "They\u2019re the butcher and grocer around these parts. Redmond\u2019s
        usually the one who has his ear to the ground, but I\u2019m sure Carl must\u2019ve
        heard a thing or two last night. "
      talkerImage: {fileID: 0}
      introduceSpeaker: 0
      isMC: 0
      removeSpeaker: 
    - speakerName: Felix
      sentenceText: "Got it! Alright Aldo, let\u2019s head out then."
      talkerImage: {fileID: 0}
      introduceSpeaker: 0
      isMC: 1
      removeSpeaker: 0001
