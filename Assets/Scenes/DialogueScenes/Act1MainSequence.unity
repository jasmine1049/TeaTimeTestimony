MonoBehaviour:
  m_ObjectHideFlags: 0
  m_CorrespondingSourceObject: {fileID: 0}
  m_PrefabInstance: {fileID: 0}
  m_PrefabAsset: {fileID: 0}
  m_GameObject: {fileID: 876046529}
  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: 521a86c7853910c439b9adc1fa9b8d55, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  dialogue:
    sentences:
    - speakerName: Aldo
      sentenceText: Ah, pardon us my good sirs.
      talkerImage: {fileID: 21300000, guid: 6404e1f10d1464d40b86bb68e3d83811, type: 3}
      introduceSpeaker: 1
      isMC: 0
      removeSpeaker: 
    - speakerName: Dietrich
      sentenceText: "Ah, you must be the two newcomers I\u2019ve heard about."
      talkerImage: {fileID: 21300000, guid: ce2e4ea64173342478f4510e8128c5ca, type: 3}
      introduceSpeaker: 1
      isMC: 0
      removeSpeaker: 
    - speakerName: Dietrich
      sentenceText: "Please, let me give you a proper welcome. I\u2019m Dietrich
        Wiesel, the soon to be mayor of Edelweiss, and this burly gentleman beside
        me is my bodyguard, Luther."
      talkerImage: {fileID: 0}
      introduceSpeaker: 0
      isMC: 0
      removeSpeaker: 
    - speakerName: Aldo
      sentenceText: "Nice to make your acquaintance. My name\u2019s Aldo, and this
        young lad here is Felix."
      talkerImage: {fileID: 21300000, guid: 6404e1f10d1464d40b86bb68e3d83811, type: 3}
      introduceSpeaker: 1
      isMC: 0
      removeSpeaker: 
    - speakerName: Felix
      sentenceText: "How\u2019s it going?"
      talkerImage: {fileID: 21300000, guid: c8b92b8c5468c3c41b25cf7656decb72, type: 3}
      introduceSpeaker: 1
      isMC: 1
      removeSpeaker: 
    - speakerName: Luther
      sentenceText: "\u2026"
      talkerImage: {fileID: 21300000, guid: 9eb3db1bad0b3704aa83acf3d0fee805, type: 3}
      introduceSpeaker: 1
      isMC: 0
      removeSpeaker: 
    - speakerName: Dietrich
      sentenceText: 'Ha ha, please forgive my partner here, what he lacks in words
        he certainly makes up for in strength. '
      talkerImage: {fileID: 21300000, guid: ce2e4ea64173342478f4510e8128c5ca, type: 3}
      introduceSpeaker: 1
      isMC: 0
      removeSpeaker: 
    - speakerName: Dietrich
      sentenceText: Why I hired him after all! Without him, Kreuz Drilling would
        not be where it is today!
      talkerImage: {fileID: 0}
      introduceSpeaker: 0
      isMC: 0
      removeSpeaker: 
